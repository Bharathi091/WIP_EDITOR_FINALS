<core:FragmentDefinition xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form" xmlns:core="sap.ui.core"
	xmlns:smartField="sap.ui.comp.smartfield" xmlns:smartMultiInput="sap.ui.comp.smartmultiinput"  xmlns:m="sap.m">
	<Dialog title="Mass Transfer">
		<l:Grid defaultSpan="L12 M12 S12" width="auto" id="idGrid">
			<l:content>
				<Label text="Target Matter"/>
				<!--<smartMultiInput:SmartMultiInput id="masspspid" entitySet="WipMattersSet" textLabel="matter" width="20%" value="{Pspid}"/>-->
				<!--<smartField:SmartField id="masspspid" entitySet="WipDetailsSet" value="{ToMatter}"/>-->
				<smartField:SmartField id="masspspid" entitySet="WipMattersSet" value="{Pspid}"/>
				<Label text="Percentage" labelFor="percentage"/>
				<Input id="percentage" value="100"/>
				<Table id="masstransfertable">
					<columns>
						<Column width="160px">
							<Text text="Document Number"/>
						</Column>
						<Column width="80px">
							<Text text="item Number"/>
						</Column>
						<Column width="80px">
							<Text text="Delete"/>
						</Column>
					</columns>
					<items></items>
				</Table>
			</l:content>
		</l:Grid>
		<buttons>
			<Button text="Transfer" press="onmassTransferchange" type="Accept"/>
			<Button text="Cancel" press="closemassDialog" type="Reject"/>
		</buttons>
	</Dialog>
</core:FragmentDefinition>