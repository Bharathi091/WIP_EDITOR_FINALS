<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
	<Dialog contentWidth="600px" contentHeight="300px" draggable="true" id="Alert" title="{i18n>Attachments}" xmlns:table="sap.ui.table">
		<content>
			<table:Table rows="{path:'attatchmentsData>/results'}" selectionMode="None" visibleRowCount="7">
				<table:columns>
					<table:Column width="15rem">
						<Label text="Created User"/>
						<table:template>
							<Text text="{attatchmentsData>CreaUser}"/>
						</table:template>
					</table:Column>
					<table:Column width="15rem">
						<Label text="Document"/>
						<table:template>
							<Link text="{attatchmentsData>Descript}.{attatchmentsData>Docuclass}" subtle="true" press="handleAttachmentsDialogEvents"
								app:selectedData="{attatchmentsData>}" app:type="link"/>
						</table:template>
					</table:Column>
					<table:Column width="8rem">
						<Label text=""/>
						<table:template>
							<Button type="Default" text="{i18n>Delete}" press="handleAttachmentsDialogEvents" 	app:selectedData="{attatchmentsData>}" app:type="delete"/>
						</table:template>
					</table:Column>
				</table:columns>
			</table:Table>
		</content>
		
		<endButton>
			<Button type="Default" text="{i18n>Cancel}" press="attachmentDialogClosedWithCancel"/>
		</endButton>
	</Dialog>
</core:FragmentDefinition>