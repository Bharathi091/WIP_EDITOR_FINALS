<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:html="http://www.w3.org/1999/xhtml">
	<Dialog contentWidth="400px" contentHeight="300px" draggable="true" id="PrintFinal" title="{i18n>PleaseSelect}">
		<content>
			<Select width="100%" forceSelection="false" selectedKey="{printfinalData>/selected}" items="{ path: 'printfinalData>/data/results'}"
				change="getbillDataMethod">
				<core:Item key="{printfinalData>Kschl}" text="{printfinalData>Vtext} - {printfinalData>Kschl}"/>
			</Select>
		</content>
		<beginButton>
			<Button type="Default" text="{i18n>Ok}" press="printDialogClosedWithOk"/>
		</beginButton>
		<endButton>
			<Button type="Default" text="{i18n>Cancel}" press="printDialogClosedWithCancel"/>
		</endButton>
	</Dialog>
</core:FragmentDefinition>