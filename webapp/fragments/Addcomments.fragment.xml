<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:html="http://www.w3.org/1999/xhtml">
	<Dialog contentWidth="600px" contentHeight="130px" draggable="true" id="Alert" title="{i18n>AddcommentsDialogTitle}">
		<content>
		
			<TextArea class = "commentsaddArea" width="100%" id="TypeHere" value="{billEditModel>/billEditSearchData/0/BlockNarrative}" valueLiveUpdate="{/ValueLiveUpdate}"
				liveChange="handleLiveChange"/>
		</content>
		<beginButton>
			<Button type="Default" text="{i18n>Ok}" press="comDialogClosedWithOk"/>
		</beginButton>
		<endButton>
			<Button type="Default" text="{i18n>Cancel}" press="comDialogClosedWithOk"/>
		</endButton>
	</Dialog>
</core:FragmentDefinition>