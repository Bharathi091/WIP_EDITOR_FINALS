<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<P13nDialog showReset="false" showResetEnabled="false" ok="onOK" cancel="onCancel" reset="onReset"
		class="sapUiSizeCompact">
		<panels>
			<P13nColumnsPanel title="Columns" changeColumnsItems="onChangeColumnsItems" items="{path: 'settings>/data'}"
				columnsItems="{path: 'settings>/ColumnsItems'}">
