					<P13nItem columnKey="{settings>columnKey}" text="{settings>text}"/>
				</items>
				<columnsItems>
					<P13nColumnsItem columnKey="{settings>columnKey}" visible="{settings>visible}"/>
				</columnsItems>
			</P13nColumnsPanel>
